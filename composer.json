{
	"name": "protoframework/proto",
	"description": "The Proto Framework - A modular monolith framework for PHP.",
	"keywords": ["proto", "framework", "modular", "monolith", "php"],
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Chris Durfee",
			"email": "j.c.durf@gmail.com"
		}
	],
	"require": {
		"php": ">=8.1",
		"minishlink/web-push": "^7.0",
		"orhanerday/open-ai": "^5.0",
		"aws/aws-sdk-php": "^3.283",
		"twilio/sdk": "^7.9"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.0",
		"fakerphp/faker": "^1.21",
		"phpstan/phpstan": "^1.10"
	},
	"scripts": {
		"test": "phpunit",
		"analyze": "phpstan analyse"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-main": "1.0-dev"
		}
	}
}
