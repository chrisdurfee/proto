{
	"name": "protoframework/proto",
	"description": "The Proto Framework - A modular monolith framework for PHP.",
	"keywords": [
		"proto",
		"framework",
		"modular",
		"monolith",
		"php"
	],
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Chris Durfee",
			"email": "j.c.durf@gmail.com"
		}
	],
	"require": {
		"php": ">=8.4",
		"aws/aws-sdk-php": "^3.356",
		"minishlink/web-push": "^9.0",
		"twilio/sdk": "^7.16",
		"web-token/jwt-library": "*",
		"phpmailer/phpmailer": "^6.10"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.5",
		"fakerphp/faker": "^1.24",
		"phpstan/phpstan": "^1.12"
	},
	"scripts": {
		"test": "phpunit",
		"analyze": "phpstan analyse"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-main": "1.0-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"Proto\\": "src/"
		},
		"classmap": [
			"src/Migrations"
		]
	}
}
